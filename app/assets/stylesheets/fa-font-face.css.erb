@font-face {
  font-family: 'FontAwesome';
  src:  url('<%= font_path 'fontawesome-webfont.eot?v=4.5.0' %>');
  src:  url('<%= font_path 'fontawesome-webfont.eot?#iefix&v=4.5.0' %>') format('embedded-opentype'), 
        url('<%= font_path 'fontawesome-webfont.woff2?v=4.5.0' %>') format('woff2'), 
        url('<%= font_path 'fontawesome-webfont.woff?v=4.5.0' %>') format('woff'), 
        url('<%= font_path 'fontawesome-webfont.ttf?v=4.5.0' %>') format('truetype'), 
        url('<%= font_path 'fontawesome-webfont.svg?v=4.5.0#fontawesomeregular' %>') format('svg');
  font-weight: normal;
  font-style: normal;
}